= form_for(@store) do |f|
  .field
    = f.label :name
    %br/
    = f.text_field :name, :disabled => true
  .field
    = f.label :expense_category
    %br/
    = select :store, :expense_categories, ExpenseCategory::CATEGORIES, {:include_blank => true, :multiple => true}
  .actions.clearer
    = f.submit