= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
  %fieldset#login_form
    %ol
      %li= f.text_field :email, :placeholder => "email@address.com"
      %li= f.password_field :password, :placeholder => "password"
      %br/
      %li= f.check_box :remember_me
      = f.label :remember_me
      = f.submit "Sign in"
%br/
%p.center= render :partial => "devise/shared/links"